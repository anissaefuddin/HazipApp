<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Hazip.ViewModels"
                    xmlns:view="clr-namespace:Hazip.Views">

    <DataTemplate DataType="{x:Type vm:OverviewVM}">
        <view:Study />
    </DataTemplate>
    

    <DataTemplate DataType="{x:Type vm:StudyVM}">
        <view:Study />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:RiskCriteriaVM}">
        <view:Risk_Criteria />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:VM_Safeguard}">
        <view:Safeguards />
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:VM_GuideWord}">
        <view:GuideWord />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VM_TeamMember}">
        <view:Study_TeamMember />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VM_Nodes}">
        <view:Nodes />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VM_Likehood}">
        <view:Risk_Criteria_Likehood_Categories />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transactions />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShipmentVM}">
        <view:Shipments />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>
    
</ResourceDictionary>